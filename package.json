{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/server/test/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.4.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.2",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.2",
    "@nestjs/swagger": "^4.5.12",
    "@types/ali-oss": "^6.0.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/crypto-js": "^3.1.47",
    "@types/lodash": "^4.14.158",
    "@types/mongoose": "^5.7.34",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/node-uuid": "^0.0.28",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "ali-oss": "^6.10.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "crypto-random-string": "^3.2.0",
    "mongoose": "^5.9.27",
    "mongoose-unique-validator": "^2.0.3",
    "multer-aliyun-oss": "^1.1.0",
    "nestjs-mongoose-crud": "^2.0.0",
    "node-uuid": "^1.4.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.4.2",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^17.1.4",
    "@types/jest": "26.0.8",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.8.0",
    "@typescript-eslint/parser": "3.8.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.2.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.4",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@libs/db/(.*)": "<rootDir>/libs/db/src/$1",
      "@libs/db": "<rootDir>/libs/db/src",
      "@libs/config/(.*)": "<rootDir>/libs/config/src/$1",
      "@libs/config": "<rootDir>/libs/config/src",
      "@app/config/(.*)": "<rootDir>/libs/config/src/$1",
      "@app/config": "<rootDir>/libs/config/src",
      "@libs/common/(.*)": "<rootDir>/libs/common/src/$1",
      "@libs/common": "<rootDir>/libs/common/src",
      "@lib/upload/(.*)": "<rootDir>/libs/upload/src/$1",
      "@lib/upload": "<rootDir>/libs/upload/src"
    }
  }
}
